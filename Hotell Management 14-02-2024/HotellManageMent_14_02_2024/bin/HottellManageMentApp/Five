package HottellManageMentApp;

import java.util.Date;

public class CustomerDetails
{
	
	private String CustomerName;
	private String CustomerStayingReson;
	private int CustomerAge;
	private String CustomerNative;
	private String CustomerCommingFrom;
	private String Distric;
	private Date Checking_Date;
	private Date CheckOut_Date;
	private double TotalAmount;
	private int Days;
	private int RoomNumber;
	
	
	
	public int getRoomNumber() {
		return RoomNumber;
	}
	public void setRoomNumber(int roomNumber) {
		RoomNumber = roomNumber;
	}
	public int getDays() {
		return Days;
	}
	public void setDays(int days) {
		Days = days;
	}
	public double getTotalAmount() {
		return TotalAmount;
	}
	public void setTotalAmount(double totalAmount) {
		TotalAmount = totalAmount;
	}
	public Date getCheckOut_Date() {
		return CheckOut_Date;
	}
	public void setCheckOut_Date(Date checkOut_Date) {
		CheckOut_Date = checkOut_Date;
	}
	public Date getChecking_Date() {
		return Checking_Date;
	}
	public void setChecking_Date(Date dt) {
		Checking_Date = dt;
	}
	public String getCustomerName() {
		return CustomerName;
	}
	public void setCustomerName(String customerName) {
		CustomerName = customerName;
	}
	public String getCustomerStayingReson() {
		return CustomerStayingReson;
	}
	public void setCustomerStayingReson(String customerStayingReson) {
		CustomerStayingReson = customerStayingReson;
	}
	public int getCustomerAge() {
		return CustomerAge;
	}
	public void setCustomerAge(int customerAge) {
		CustomerAge = customerAge;
	}
	public String getCustomerNative() {
		return CustomerNative;
	}
	public void setCustomerNative(String customerNative) {
		CustomerNative = customerNative;
	}
	public String getCustomerCommingFrom() {
		return CustomerCommingFrom;
	}
	public void setCustomerCommingFrom(String customerCommingFrom) {
		CustomerCommingFrom = customerCommingFrom;
	}
	public String getDistric() {
		return Distric;
	}
	public void setDistric(String distric) {
		Distric = distric;
	}

	
	
